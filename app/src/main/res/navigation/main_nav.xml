<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/bottom_nav_graph"
    app:startDestination="@id/homeFragment">

    <activity
        android:id="@+id/homeActivity"
        android:name="com.kianmahmoudi.android.shirazgard.activities.HomeActivity"
        android:label="HomeActivity">
        <argument
            android:name="destination"
            app:argType="string" />
        <argument
            android:name="latitude"
            app:argType="float" />
        <argument
            android:name="longitude"
            app:argType="float" />
    </activity>


    <fragment
        android:id="@+id/homeFragment"
        android:name="com.kianmahmoudi.android.shirazgard.fragments.Home.HomeFragment"
        android:label="HomeFragment">
        <action
            android:id="@+id/action_homeFragment_to_categoryPlacesFragment"
            app:destination="@id/categoryPlacesFragment"
            app:enterAnim="@anim/slide_in_down"
            app:exitAnim="@anim/slide_out_up"
            app:popEnterAnim="@anim/slide_in_up"
            app:popExitAnim="@anim/slide_out_down" />
        <action
            android:id="@+id/action_homeFragment_to_placeDetailsFragment"
            app:destination="@id/placeDetailsFragment"
            app:enterAnim="@anim/slide_in_down"
            app:exitAnim="@anim/slide_out_up"
            app:popEnterAnim="@anim/slide_in_up"
            app:popExitAnim="@anim/slide_out_down" />
        <action
            android:id="@+id/action_homeFragment_to_categoriesFragment"
            app:destination="@id/categoriesFragment"
            app:enterAnim="@anim/slide_in_down"
            app:exitAnim="@anim/slide_out_up"
            app:popEnterAnim="@anim/slide_in_up"
            app:popExitAnim="@anim/slide_out_down"
            app:popUpTo="@id/homeFragment"
            app:popUpToSaveState="true"
            app:restoreState="true" />
    </fragment>
    <fragment
        android:id="@+id/favoriteFragment"
        android:name="com.kianmahmoudi.android.shirazgard.fragments.Home.FavoriteFragment"
        android:label="FavoriteFragment">
        <action
            android:id="@+id/action_favoriteFragment_to_placeDetailsFragment"
            app:destination="@id/placeDetailsFragment"
            app:enterAnim="@anim/slide_in_down"
            app:exitAnim="@anim/slide_out_up"
            app:popEnterAnim="@anim/slide_in_up"
            app:popExitAnim="@anim/slide_out_down" />
    </fragment>
    <fragment
        android:id="@+id/mapFragment"
        android:name="com.kianmahmoudi.android.shirazgard.fragments.Home.MapFragment"
        android:label="MapFragment" />
    <fragment
        android:id="@+id/categoriesFragment"
        android:name="com.kianmahmoudi.android.shirazgard.fragments.CategoriesFragment"
        android:label="CategoriesFragment">
        <action
            android:id="@+id/action_categoriesFragment_to_categoryPlacesFragment"
            app:destination="@id/categoryPlacesFragment"
            app:enterAnim="@anim/slide_in_down"
            app:exitAnim="@anim/slide_out_up"
            app:popEnterAnim="@anim/slide_in_up"
            app:popExitAnim="@anim/slide_out_down" />
    </fragment>
    <fragment
        android:id="@+id/settingFragment"
        android:name="com.kianmahmoudi.android.shirazgard.fragments.Home.SettingFragment"
        android:label="SettingFragment">
        <action
            android:id="@+id/action_settingFragment_to_fragmentChangePassword"
            app:destination="@id/fragmentChangePassword"
            app:enterAnim="@anim/slide_in_down"
            app:exitAnim="@anim/slide_out_up"
            app:popEnterAnim="@anim/slide_in_up"
            app:popExitAnim="@anim/slide_out_down" />
        <action
            android:id="@+id/action_settingFragment_to_fragmentIntroduction2"
            app:destination="@id/fragmentIntroduction2"
            app:enterAnim="@anim/slide_in_down"
            app:exitAnim="@anim/slide_out_up"
            app:popEnterAnim="@anim/slide_in_up"
            app:popExitAnim="@anim/slide_out_down" />
        <action
            android:id="@+id/action_settingFragment_to_editProfileFragment"
            app:destination="@id/editProfileFragment"
            app:enterAnim="@anim/slide_in_down"
            app:exitAnim="@anim/slide_out_up"
            app:popEnterAnim="@anim/slide_in_up"
            app:popExitAnim="@anim/slide_out_down"/>
    </fragment>
    <fragment
        android:id="@+id/categoryPlacesFragment"
        android:name="com.kianmahmoudi.android.shirazgard.fragments.CategoryPlacesFragment"
        android:label="CategoryPlacesFragment">
        <argument
            android:name="categoryName"
            app:argType="string" />
        <argument
            android:name="categoryType"
            app:argType="string" />
        <action
            android:id="@+id/action_categoryPlacesFragment_to_placeDetailsFragment"
            app:destination="@id/placeDetailsFragment"
            app:enterAnim="@anim/slide_in_down"
            app:exitAnim="@anim/slide_out_up"
            app:popEnterAnim="@anim/slide_in_up"
            app:popExitAnim="@anim/slide_out_down" />
    </fragment>
    <fragment
        android:id="@+id/placeDetailsFragment"
        android:name="com.kianmahmoudi.android.shirazgard.fragments.PlaceDetailsFragment"
        android:label="PlaceDetailsFragment">
        <argument
            android:name="enName"
            app:argType="string" />
        <argument
            android:name="faName"
            app:argType="string" />
        <argument
            android:name="address"
            app:argType="string" />
        <argument
            android:name="description"
            app:argType="string" />
        <argument
            android:name="type"
            app:argType="string" />
        <argument
            android:name="latitude"
            app:argType="float" />
        <argument
            android:name="longitude"
            app:argType="float" />
        <argument
            android:name="images"
            app:argType="string[]" />
        <action
            android:id="@+id/action_placeDetailsFragment_to_homeActivity"
            app:destination="@id/homeActivity"
            app:enterAnim="@anim/slide_in_down"
            app:exitAnim="@anim/slide_out_up"
            app:popEnterAnim="@anim/slide_in_up"
            app:popExitAnim="@anim/slide_out_down" />
        <argument
            android:name="objectId"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/searchFragment"
        android:name="com.kianmahmoudi.android.shirazgard.fragments.SearchFragment"
        android:label="SearchFragment">
        <action
            android:id="@+id/action_searchFragment_to_placeDetailsFragment"
            app:destination="@id/placeDetailsFragment"
            app:enterAnim="@anim/slide_in_down"
            app:exitAnim="@anim/slide_out_up"
            app:popEnterAnim="@anim/slide_in_up"
            app:popExitAnim="@anim/slide_out_down" />
    </fragment>
    <fragment
        android:id="@+id/fragmentChangePassword"
        android:name="com.kianmahmoudi.android.shirazgard.fragments.auth.FragmentChangePassword"
        android:label="FragmentChangePassword" />
    <fragment
        android:id="@+id/fragmentIntroduction2"
        android:name="com.kianmahmoudi.android.shirazgard.fragments.start.FragmentIntroduction"
        android:label="FragmentIntroduction" />
    <fragment
        android:id="@+id/editProfileFragment"
        android:name="com.kianmahmoudi.android.shirazgard.fragments.EditProfileFragment"
        android:label="EditProfileFragment" />

</navigation>